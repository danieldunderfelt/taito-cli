{
  "name": "taito-cli",
  "version": "0.1.0",
  "description": "CLI for installing customizable Agent Skills from GitHub.",
  "type": "module",
  "bin": {
    "taito": "./bin/taito.ts"
  },
  "scripts": {
    "dev": "bun run bin/taito.ts",
    "build": "bun build ./bin/taito.ts --outdir ./dist --target node",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clack/prompts": "^0.9.1",
    "commander": "^13.1.0",
    "ejs": "^3.1.10",
    "smol-toml": "^1.3.1",
    "tar": "^7.4.3"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@types/bun": "^1.2.4",
    "@types/ejs": "^3.1.5",
    "prettier": "^3.8.0",
    "typescript": "^5.7.3"
  }
}
